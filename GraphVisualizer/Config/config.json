{
    "title": "Configurations",
    "groups": {
        "filtering": {
            "id": "filtering",
            "description": "Filtering",
            "data": {
                "allEdges": {
                    "id": "allEdges",
                    "description": "Show all Edges",
                    "type": "checkbox",
                    "value": true,
                    "conditions": {},
                    "onChange": {
                        "event": "allEdges"
                    }
                },
                "weightFilter": {
                    "id": "weightFilter",
                    "description": "Filter via Weights",
                    "type": "slider",
                    "value": 1,
                    "min": 1,
                    "max": 100,
                    "conditions": {
                        "disabled": "this.groups.filtering.data.allEdges.value === true"
                    },
                    "onChange": {
                        "event": "weightFilter"
                    }
                },
                "showWeights": {
                    "id": "showWeights",
                    "description": "Show Weights",
                    "type": "checkbox",
                    "value": true,
                    "conditions": {},
                    "onChange": {
                        "event": "showWeights"
                    }
                }
            }
        },
        "grouping": {
            "id": "grouping",
            "description": "Grouping",
            "data": {
                "groupingBy": {
                    "id": "groupingBy",
                    "description": "Grouping By",
                    "type": "select",
                    "value": "none",
                    "options": {
                        "none": {
                            "id": "none",
                            "description": "---",
                            "value": "none"
                        },
                        "namespace": {
                            "id": "namespace",
                            "description": "Namespace",
                            "value": "namespace"
                        },
                        "fileName": {
                            "id": "fileName",
                            "description": "File",
                            "value": "fileName"
                        },
                        "louvain": {
                            "id": "louvain",
                            "description": "Louvain",
                            "value": "louvain"
                        }
                    },
                    "conditions": {},
                    "onChange": {
                        "event": "groupingBy"
                    }
                }
            }
        },
        "general": {
            "id": "general",
            "description": "General",
            "data": {
                "cycles": {
                    "id": "cycles",
                    "description": "Show Cycles",
                    "type": "checkbox",
                    "value": false,
                    "conditions": {
                        "disabled": "this.groups.filtering.data.allEdges.value === false"
                    },
                    "onChange": {
                        "event": "cycles"
                    }
                }
            }
        },
        "dependenciesConfig": {
            "id": "dependenciesConfig",
            "description": "Dependencies' Configurations",
            "data": {
                "Inherite": {
                    "id": "Inherite",
                    "description": "Inheritence",
                    "type": "select",
                    "value": "default",
                    "options": {
                        "none": {
                            "id": "default",
                            "description": "Default",
                            "value": "default"
                        },
                        "weighted": {
                            "id": "weighted",
                            "description": "Apply Weight",
                            "value": "weighted"
                        },
                        "singleWeight": {
                            "id": "singleWeight",
                            "description": "Computed as Single Dependency",
                            "value": "singleWeight"
                        }
                    },
                    "conditions": {},
                    "onChange": {}
                },
                "InheriteWeight": {
                    "id": "InheriteWeight",
                    "description": "Weight",
                    "type": "number",
                    "value": 1,
                    "min": 0,
                    "max": 2,
                    "step": 0.05,
                    "conditions": {
                        "disabled": "this.groups.dependenciesConfig.data.Inherite.value !== \"weighted\""
                    },
                    "onChange": {}
                },
                "apply": {
                    "id": "apply",
                    "description": "Apply",
                    "type": "applyButton",
                    "conditions": {}, 
                    "onClick": {
                        "event" : "dependenciesConfig"
                    },
                    "onGroupData": [
                        "Inherite", 
                        "InheriteWeight"
                    ]
                   
                }
            }
        }
    }
}