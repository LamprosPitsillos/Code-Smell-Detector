{
    "title": "Configurations",
    "groups": {
        "general": {
            "id": "general",
            "description": "General",
            "data": {
                "edges": {
                    "id": "edges",
                    "description": "Show all Edges",
                    "type": "checkbox",
                    "value": true,
                    "conditions": {}
                },
                "weights": {
                    "id": "weights",
                    "description": "Filter via Weights",
                    "type": "slider",
                    "value": 5,
                    "min": 1,
                    "max": 10,
                    "conditions": {
                        "disabled": "this.groups.general.data.edges.value === true"
                    }
                },
                "cycles": {
                    "id": "cycles",
                    "description": "Show Cycles",
                    "type": "checkbox",
                    "value": false,
                    "conditions": {
                        "disabled": "this.groups.general.data.edges.value === true"
                    }
                }
            }
        }
    }
}